{
  "name": "node-evaporate",
  "version": "0.0.0-semantically-released",
  "description": "A simple, convention-based CloudFormation Stack deployment tool. The deployment region is read from the environment variable AWS_REGION or retrieved from the Instance Identity Document (for EC2 instances), otherwise it defaults to us-east-1.",
  "repository": {
    "type": "git",
    "url": "git@github.com:seek-oss/evaporate.git"
  },
  "author": "Simon Pilepich <spilepich@seek.com.au>",
  "license": "MIT",
  "main": "lib/commonjs",
  "module": "lib/module",
  "typings": "lib/index.d.ts",
  "sideEffects": false,
  "scripts": {
    "build": "smt build",
    "commit": "smt commit",
    "configure": "smt configure",
    "format": "smt format",
    "format:check": "smt format check",
    "lint": "smt lint",
    "prerelease": "smt build",
    "release": "smt release",
    "test": "jest --no-cache",
    "test:watch": "yarn test --watch"
  },
  "dependencies": {
    "aws-sdk": "^2.678.0",
    "commander": "^5.0.0",
    "jsonfile": "^6.0.1",
    "yaml": "^1.10.0"
  },
  "devDependencies": {
    "@seek/seek-module-toolkit": "^3.0.16",
    "@types/jest": "^25.2.1",
    "@types/jsonfile": "^5.0.0",
    "@types/node": "^13.11.0"
  }
}
